#!/bin/sh
set -e

# shellcheck disable=SC1091
. ./.env

node_modules/.bin/tsc
npm run build:min

.venv/bin/python manage.py collectstatic --noinput
.venv/bin/python manage.py migrate

HIVEMIND_PROCESSES=web,worker exec /home/ben/.local/share/go/bin/hivemind --title booklogger >/home/ben/src/booklogger/logs/booklogger.log 2>/home/ben/src/booklogger/logs/booklogger.err
